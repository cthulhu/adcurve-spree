<h1><%= t('spree.adcurve') %></h1>

<br />
<br />
<% if @adcurve_tag.new_record? %>
  <%= form_for [:admin, @adcurve_tag] do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>
  <% end %>
<% else %>
  <%= form_for [:admin, @adcurve_tag], :method => :put do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>
  <% end %>
<% end %>
